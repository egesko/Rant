    <ImageView
            android:layout_width="47dp"
            android:layout_height="44dp" android:background="@drawable/nav_create_icon"
            android:layout_alignParentRight="true" android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true" android:layout_marginEnd="42dp" android:layout_marginRight="42dp"
            android:id="@+id/imageView69" android:layout_marginTop="39dp"
            android:visibility="visible"/>


</RelativeLayout>